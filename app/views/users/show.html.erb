<div class="row">
  <div class="col s3">
    <%= render partial: "commons/profile", locals: { user: @user } %>
    <% if user_signed_in? && @user.id==current_user.id %>
      <%= render "commons/collection" %>
    <% end %>
  </div>
  
  <div class="col s9">
    <%= render "commons/tabs" %>
    <%= render partial: "commons/articles_list", locals: { posts: @posts } %>
  </div>
</div>


